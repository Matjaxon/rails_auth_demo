<h1>Login</h1>

  <form action="<%= session_url %>" method="POST">
    <%= auth_token %>

    <label>
      Username:
      <input type="text" name="user[user_name]" value= "<%= @user.user_name %>" >
    </label>
    <br>

    <label>
      Password:
      <input type="password" name="user[password]">
    </label>

    <input type="submit" value="Login">
  </form>
